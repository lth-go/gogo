int print(string str);

int println(string str) {
    print(str + "\n");
}

class Printable {
    void print() {}
}

class Shape : Printable {
    int color;

    void draw() {}
}

class Line : Shape {
    float  start_x;
    float  start_y;
    float  end_x;
    float  end_y;

    void draw() {
        println("draw Line!");
    }

    void print() {
        println("(" + this.start_x + ", " + this.end_x + ")-(" + this.end_x + ", " + this.end_y + ")");
    }

    void init(float start_x, float start_y, float end_x, float end_y) {
        this.start_x = start_x;
        this.start_y = start_y;
        this.end_x = end_x;
        this.end_y = end_y;
    }
}

var l Line = new Line(10, 10, 20, 20);

l.draw();
l.print();
