func print(str string) int;

func println(str string) int {
    print(str + "\n");
}

class Line {
    start_x float;
    start_y float;
    end_x float;
    end_y float;

    func draw() void {
        println("draw Line!");
    }

    func print() void {
        println("(" + this.start_x + ", " + this.end_x + ")-(" + this.end_x + ", " + this.end_y + ")");
    }

    func init(start_x float, start_y float, end_x float, end_y float) void {
        this.start_x = start_x;
        this.start_y = start_y;
        this.end_x = end_x;
        this.end_y = end_y;
    }
}

var l Line = new Line(10, 10, 20, 20);

l.draw();
l.print();
